graph G {
    x1 [shape=ellipse];
    x2 [shape=ellipse];
    x3 [shape=ellipse];
    x4 [shape=ellipse];
    i1 [shape=point, width=0, height=0, label=""];
    i2 [shape=point, width=0, height=0, label=""];
    // Création des arêtes
    x1 -- i1 [label="e1"];
    i1 -- x2;
    x1 -- i2 [label="e2"];
    i2 -- x2;
    x1 -- x2 [label="e3"];
    x1 -- x3 [label="e4"];
    x2 -- x3 [label="e5"];
    x2 -- x2 [label="e6"]; // Boucle  sur x2
    x3 -- x4 [label="e7"];
}
